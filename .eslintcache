)\r\n\r\n    dispatch(resetPassword({email, error}))\r\n  }\r\n\r\n  return (\r\n    <Form onSubmit={handleSubmit}>\r\n\r\n      {error.length > 0 && \r\n      <ul>\r\n        {error.map((e, i) => {\r\n          return (\r\n            <li key={i}>{e}</li>\r\n          )\r\n        })}\r\n      </ul> }\r\n\r\n      <FormInput \r\n        type='email'\r\n        name='email'\r\n        value={email}\r\n        placeholder='Your Email'\r\n        handleChange={(e) => setEmail(e.target.value)}\r\n      />\r\n      <Button className='btn' type='submit'>\r\n        Reset Password\r\n      </Button>\r\n    </Form>\r\n  )\r\n}\r\n\r\nexport default withRouter(ForgetPassword);\r\n","D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\redux\\createStore.js",["201"],"import { createStore, applyMiddleware} from 'redux'\r\nimport logger from 'redux-logger'\r\nimport thunk from 'redux-thunk'\r\nimport {persistStore} from 'redux-persist'\r\nimport rootReducer from './rootReducer'\r\n\r\nexport const middleware = [thunk, logger]\r\n\r\nexport const store = createStore(rootReducer, applyMiddleware(...middleware))\r\n\r\nexport const persistor = persistStore(store)\r\n\r\nexport default {\r\n  store,\r\n  persistor\r\n}","D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\redux\\rootReducer.js",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\pages\\Authentification\\Authentification.jsx",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\pages\\Homepage\\Homepage.jsx",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\pages\\Recovery\\Recovery.jsx",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\redux\\Users\\user.reducer.js",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\redux\\Users\\user.types.js",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\redux\\Users\\user.actions.js",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\pages\\Dashboard\\Dashboard.jsx",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\hoc\\WithAuth.jsx",[],"D:\\01 Dev\\03 React\\react-firebase