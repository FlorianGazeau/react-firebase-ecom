e('');\r\n  const { products } = useSelector(MapState)\r\n  const { data, queryDoc } = products\r\n  \r\n  useEffect(() => {\r\n    dispatch(fetchProducts({ filterType }))\r\n  }, [filterType]);\r\n  \r\n  const handleFilter = (e) => {\r\n    const nextFilter = e.target.value\r\n    setFilterType(nextFilter)\r\n  }\r\n  \r\n  const handleLoadMore = () => {\r\n    console.log(queryDoc)\r\n    dispatch(\r\n      fetchProducts({ \r\n        filterType, \r\n        startAfterDoc: queryDoc,\r\n        persitProducts: data   \r\n      })\r\n    )\r\n  }\r\n\r\n  const configLoadMore = {\r\n    onLoadMoreEvt: handleLoadMore,\r\n  }\r\n\r\n  if (data && data.length < 1) {\r\n    return (\r\n      <div className='searchPage'>\r\n        <div>\r\n          <select name=\"\" id=\"\" onChange={handleFilter}>\r\n            {/* <option value=\"\">Show All</option> */}\r\n            <option value=\"mens\">Mens</option>\r\n            <option value=\"womens\">Womens</option>\r\n          </select>\r\n        </div>\r\n        <h4>No search result</h4>\r\n      </div>\r\n    )\r\n  }\r\n  return (\r\n\r\n    <div className='searchPage'>\r\n\r\n      <div>\r\n        <select name=\"\" id=\"\" onChange={handleFilter}>\r\n          <option value=\"\">Show All</option>\r\n          <option value=\"mens\">Mens</option>\r\n          <option value=\"womens\">Womens</option>\r\n        </select>\r\n      </div>\r\n      <div className='result'>\r\n        {data && data.map((data, i) => {\r\n          const { img, price, name, documentID } = data\r\n\r\n          const configProduct = {\r\n            img,\r\n            price,\r\n            name, \r\n            documentID\r\n          }\r\n\r\n          return (\r\n            <CardProduct key={i} {...configProduct} />\r\n          )\r\n        })}\r\n\r\n      </div>\r\n      <LoadMore {...configLoadMore} />\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Search;\r\n","D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\components\\CardPr