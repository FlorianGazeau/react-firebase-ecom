='/admin'>\n          {currentUser && CheckAdminUser(currentUser) ? <Admin /> : <Redirect to='/'></Redirect>}\n        </Route>\n        <WithAuth>\n          <Route path='/account/profil' component={Dashboard} />\n        </WithAuth>\n      </Switch>\n    </Router>\n  );\n}\n\nconst mapStateToProps = ({ user }) => ({\n  currentUser: user.currentUser\n})\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n})\n\nexport default connect(mapStateToProps, mapDispatchToProps)(App);\n","D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\components\\Header\\Header.jsx",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\components\\Directory\\Directory.jsx",["151","152"],"import React from 'react'\r\nimport ShopWomen from '../../assets/ShopWomen.jpg'\r\nimport ShopMen from '../../assets/shopMan.jpg'\r\nimport './Directory.css'\r\n\r\nexport default function Directory() {\r\n  return (\r\n        <div className='directory'>\r\n          <div className='directory-item' style={{\r\n            backgroundImage: `url(${ShopWomen})`\r\n          }}>\r\n            <a className='btn btn-hero' href=\"\">SHOP WOMENS</a>\r\n          </div>\r\n        <div className='directory-item' style={{\r\n          backgroundImage: `url(${ShopMen})`\r\n        }}>\r\n          <a className='btn btn-hero' href=\"\">SHOP MENS</a>\r\n        </div>\r\n      </div>\r\n  )\r\n}\r\n","D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\components\\Form\\Button\\Button.jsx",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\Firebase\\utils.js",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\Firebase\\config.js",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\components\\SignIn\\SignIn.jsx",[],["153","154"],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\components\\Form\\FormInput\\FormInput.jsx",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\components\\Form\\Form\\Form.jsx",[],"D:\\01 Dev\\03 React\\react-firebase-ecom\\src\\components\\For